<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhangbin.yun.yunrights.modules.logging.mapper.LogMapper">
    <resultMap id="BaseResultMap" type="com.zhangbin.yun.yunrights.modules.logging.model.$do.LogDO">
        <id column="l_id" jdbcType="BIGINT" property="id"/>
        <result column="l_operation_desc" jdbcType="VARCHAR" property="operationDesc"/>
        <result column="l_log_level" jdbcType="CHAR" property="logLevel"/>
        <result column="l_request_method" jdbcType="VARCHAR" property="requestMethod"/>
        <result column="l_request_params" jdbcType="VARCHAR" property="requestParams"/>
        <result column="l_client_ip" jdbcType="VARCHAR" property="clientIp"/>
        <result column="l_request_time_consuming" jdbcType="BIGINT" property="requestTimeConsuming"/>
        <result column="l_user_name" jdbcType="VARCHAR" property="userName"/>
        <result column="l_address" jdbcType="VARCHAR" property="address"/>
        <result column="l_browser" jdbcType="VARCHAR" property="browser"/>
        <result column="l_exception_detail" jdbcType="VARCHAR" property="exceptionDetail"/>
        <result column="l_create_time" jdbcType="TIMESTAMP" property="createTime"/>
    </resultMap>

    <sql id="Base_Column_List">
    l_id, l_operation_desc, l_log_level, l_request_method, l_request_params, l_client_ip,
    l_request_time_consuming, l_user_name, l_address, l_browser, l_exception_detail,
    l_create_time
    </sql>

    <select id="selectByPrimaryKey" parameterType="java.lang.Long" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from t_sys_log
        where l_id = #{id,jdbcType=BIGINT}
    </select>

    <select id="selectAllByCriteria" parameterType="com.zhangbin.yun.yunrights.modules.logging.model.criteria.LogQueryCriteria"
            resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from t_sys_log
        <trim prefix="where" prefixOverrides="and | or">
            <if test='criteria != null'>
                <if test='criteria.blurry != ""'>
                    <if test='queryInfo.blurryType == "1"'>
                        and l_user_name like concat('%', #{criteria.blurry}, '%')
                    </if>
                    <if test='criteria.blurryType == "2"'>
                        and l_operation_desc like concat('%', #{criteria.blurry}, '%')
                    </if>
                    <if test='criteria.blurryType == "3"'>
                        and l_address like concat('%', #{criteria.blurry}, '%')
                    </if>
                    <if test='criteria.blurryType == "4"'>
                        and l_client_ip like concat('%', #{criteria.blurry}, '%')
                    </if>
                    <if test='criteria.blurryType == "5"'>
                        and l_request_method like concat('%', #{criteria.blurry}, '%')
                    </if>
                    <if test='criteria.blurryType == "6"'>
                        and l_request_params like concat('%', #{criteria.blurry}, '%')
                    </if>
                </if>
                <if test='criteria.logLevel != null and criteria.logLevel != ""'>
                    and l_log_level in (${criteria.logLevel})
                </if>
                <if test='criteria.createTimes != null and criteria.createTimes > 0'>
                    and l_create_time
                    <foreach collection="createTimes" item="createTime" open=" between " separator=" and " close="">
                        #{createTime}
                    </foreach>
                </if>
            </if>
        </trim>
    </select>

    <delete id="deleteByPrimaryKey" parameterType="java.lang.Long">
      delete from t_sys_log
      where l_id = #{id,jdbcType=BIGINT}
    </delete>

    <delete id="deleteByLogLevel" parameterType="java.lang.String">
      delete from t_sys_log
      where l_log_level = #{logLevel,jdbcType=VARCHAR}
    </delete>

    <insert id="insert" parameterType="com.zhangbin.yun.yunrights.modules.logging.model.$do.LogDO">
        <selectKey keyProperty="id" order="BEFORE" resultType="java.lang.Long">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into t_sys_log (l_id, l_operation_desc, l_log_level,
        l_request_method, l_request_params, l_client_ip,
        l_request_time_consuming, l_user_name, l_address,
        l_browser, l_exception_detail, l_create_time
        )
        values (#{id,jdbcType=BIGINT}, #{operationDesc,jdbcType=VARCHAR}, #{logLevel,jdbcType=CHAR},
        #{requestMethod,jdbcType=VARCHAR}, #{requestParams,jdbcType=VARCHAR}, #{clientIp,jdbcType=VARCHAR},
        #{requestTimeConsuming,jdbcType=BIGINT}, #{userName,jdbcType=VARCHAR}, #{address,jdbcType=VARCHAR},
        #{browser,jdbcType=VARCHAR}, #{exceptionDetail,jdbcType=VARCHAR}, #{createTime,jdbcType=TIMESTAMP}
        )
    </insert>
    <insert id="insertSelective" parameterType="com.zhangbin.yun.yunrights.modules.logging.model.$do.LogDO">
        <selectKey keyProperty="id" order="BEFORE" resultType="java.lang.Long">
            SELECT LAST_INSERT_ID()
        </selectKey>
        insert into t_sys_log
        <trim prefix="(" suffix=")" suffixOverrides=",">
            l_id,
            <if test="operationDesc != null">
                l_operation_desc,
            </if>
            <if test="logLevel != null">
                l_log_level,
            </if>
            <if test="requestMethod != null">
                l_request_method,
            </if>
            <if test="requestParams != null">
                l_request_params,
            </if>
            <if test="clientIp != null">
                l_client_ip,
            </if>
            <if test="requestTimeConsuming != null">
                l_request_time_consuming,
            </if>
            <if test="userName != null">
                l_user_name,
            </if>
            <if test="address != null">
                l_address,
            </if>
            <if test="browser != null">
                l_browser,
            </if>
            <if test="exceptionDetail != null">
                l_exception_detail,
            </if>
            <if test="createTime != null">
                l_create_time,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            #{id,jdbcType=BIGINT},
            <if test="operationDesc != null">
                #{operationDesc,jdbcType=VARCHAR},
            </if>
            <if test="logLevel != null">
                #{logLevel,jdbcType=CHAR},
            </if>
            <if test="requestMethod != null">
                #{requestMethod,jdbcType=VARCHAR},
            </if>
            <if test="requestParams != null">
                #{requestParams,jdbcType=VARCHAR},
            </if>
            <if test="clientIp != null">
                #{clientIp,jdbcType=VARCHAR},
            </if>
            <if test="requestTimeConsuming != null">
                #{requestTimeConsuming,jdbcType=BIGINT},
            </if>
            <if test="userName != null">
                #{userName,jdbcType=VARCHAR},
            </if>
            <if test="address != null">
                #{address,jdbcType=VARCHAR},
            </if>
            <if test="browser != null">
                #{browser,jdbcType=VARCHAR},
            </if>
            <if test="exceptionDetail != null">
                #{exceptionDetail,jdbcType=VARCHAR},
            </if>
            <if test="createTime != null">
                #{createTime,jdbcType=TIMESTAMP},
            </if>
        </trim>
    </insert>
</mapper>
